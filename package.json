{
  "name": "vue-xstate",
  "version": "1.0.4",
  "description": "xState implementation for VueJS as Mixin",
  "author": "Aram Sabatés Bermúdez",
  "scripts": {
    "build": "tsc -p tsconfig.prod.json && uglifyjs --compress --mangle --output lib/index.js  -- lib/index.js",
    "test": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint",
    "postversion": "git push && git push --tags",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint && npm run build"
  },
  "main": "lib/index.js",
  "files": [
    "lib/**/*"
  ],
  "dependencies": {
    "core-js": "^2.6.5",
    "rxjs": "^6.5.2",
    "uuid": "^3.3.2",
    "vue": "^2.6.10",
    "vue-property-decorator": "^8.1.0",
    "xstate": "^4.6.7"
  },
  "devDependencies": {
    "@types/jest": "^23.1.4",
    "@types/uuid": "^3.4.5",
    "@vue/cli-plugin-babel": "^3.9.0",
    "@vue/cli-plugin-typescript": "^3.9.0",
    "@vue/cli-plugin-unit-jest": "^3.9.0",
    "@vue/cli-service": "^3.9.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-core": "7.0.0-bridge.0",
    "ts-jest": "^23.0.0",
    "tsc": "^1.20150623.0",
    "typescript": "^3.4.3",
    "uglify-es": "^3.3.9",
    "vue-template-compiler": "^2.6.10"
  },
  "homepage": "https://github.com/asbermudez/vue-xstate",
  "keywords": [
    "xstate",
    "state machine",
    "vue",
    "vuejs",
    "mixin",
    "mixins"
  ],
  "license": "MIT",
  "repository": "https://github.com/asbermudez/vue-xstate",
  "types": "lib/index.d.ts"
}
