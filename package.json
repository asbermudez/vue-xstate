{
  "name": "vue-xstate",
  "version": "1.0.10",
  "description": "xState implementation for VueJS as Mixin",
  "author": "Aram Sabatés Bermúdez",
  "scripts": {
    "build": "parcel build src/index.ts --out-dir lib/ && tsc --emitDeclarationOnly true",
    "test": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint",
    "postversion": "git push && git push --tags",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint"
  },
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "files": [
    "build/**/*"
  ],
  "dependencies": {
    "core-js": "^3.6.4",
    "rxjs": "^6.5.5",
    "uuid": "^7.0.3",
    "vue": "^2.6.11",
    "vue-class-component": "^7.2.3",
    "vue-property-decorator": "^8.4.1",
    "xstate": "^4.9.1"
  },
  "peerDependencies": {
    "vue": "^2.5.13"
  },
  "devDependencies": {
    "@types/jest": "^24.0.19",
    "@types/uuid": "^7.0.2",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "@vue/cli-plugin-babel": "^4.3.0",
    "@vue/cli-plugin-eslint": "^4.3.0",
    "@vue/cli-plugin-typescript": "^4.3.0",
    "@vue/cli-plugin-unit-jest": "^4.3.0",
    "@vue/cli-service": "^4.3.0",
    "@vue/eslint-config-airbnb": "^5.0.2",
    "@vue/eslint-config-typescript": "^5.0.2",
    "@vue/test-utils": "1.0.0-beta.31",
    "eslint": "^6.7.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-vue": "^6.2.2",
    "parcel": "^1.12.4",
    "sass": "^1.26.3",
    "sass-loader": "^8.0.2",
    "typescript": "~3.8.3",
    "vue-template-compiler": "^2.6.11"
  },
  "homepage": "https://github.com/asbermudez/vue-xstate",
  "keywords": [
    "xstate",
    "state machine",
    "vue",
    "vuejs",
    "mixin",
    "mixins"
  ],
  "license": "MIT",
  "repository": "https://github.com/asbermudez/vue-xstate"
}
